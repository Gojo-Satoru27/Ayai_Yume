local validKeys = {
    "1",
    "KEY2",
    "KEY3"
}

if not getgenv().Key then
    game:GetService("Players").LocalPlayer:Kick("Bạn chưa có key! Vui lòng nhập key trước khi chạy script.")
    return
end

local function checkKey()
    for _, key in pairs(validKeys) do
        if getgenv().Key == key then
            return true
        end
    end
    return false
end

if checkKey() then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gojo-Satoru27/Ayai_Yume/refs/heads/main/AOTUAIM"))()
else
    game:GetService("Players").LocalPlayer:Kick("Key không hợp lệ! Hãy lấy key hợp lệ để tiếp tục.")
end