    Title = "Reborn",
    Description = "No Key",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/WUzVnZmB"))()
    end
})

Tabs.MS:AddButton({
    Title = "Meme Sea",
    Description = "No Key",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Scripts/MemeSea.lua"))()
    end
