-- Tải và chạy thư viện Fluent
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

-- Tải các tiện ích bổ sung
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Tạo cửa sổ chính
local Window = Fluent:CreateWindow({
    Title = "Script Tổng Hợp",
    SubTitle = "made by Thong",
    TabWidth = 160,
    Size = UDim2.fromOffset(500, 360),
    Acrylic = true,
    Theme = "Dark", -- Đặt chủ đề
    MinimizeKey = Enum.KeyCode.End
})

-- Tạo các tab
local Tabs = {
    ALL = Window:AddTab({ Title = "All Map" }),
    BloxFruits = Window:AddTab({ Title = "Blox Fruits" }),
    KL = Window:AddTab({ Title = "King Legacy" }),
    AD = Window:AddTab({ Title = "Anime Defense" }),
    AV = Window:AddTab({ Title = "Anime Vanguard" }),
    AR = Window:AddTab({ Title = "Reborn" }),
    MS = Window:AddTab({ Title = "Meme Sea" }),
    D = Window:AddTab({ Title = "Doors" }),
    F = Window:AddTab({ Title = "Fisch" }),
    PG = Window:AddTab({ Title = "Pets Go" }),
    E = Window:AddTab({ Title = "Evade" })
}

-- Tạo GUI chính
local ScreenGui = Instance.new("ScreenGui")
local ImageButton = Instance.new("ImageButton")
local UICorner = Instance.new("UICorner")
local CopyButton = Instance.new("TextButton") -- Nút sao chép link Discord

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Cài đặt nút hiện có
ImageButton.Parent = ScreenGui
ImageButton.BackgroundTransparency = 1
ImageButton.Image = "rbxassetid://91409498103068"
ImageButton.BorderSizePixel = 0
ImageButton.Position = UDim2.new(0.10083337, 0, 0.09, 0)
ImageButton.Size = UDim2.new(0, 50, 0, 50)
ImageButton.Draggable = true
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = ImageButton

ImageButton.MouseButton1Down:Connect(function()
    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.End, false, game)
end)

-- Cài đặt nút sao chép link Discord
CopyButton.Parent = ScreenGui
CopyButton.BackgroundColor3 = Color3.new(0.36, 0.36, 0.36)
CopyButton.Position = UDim2.new(0, 20, 0.9, 0) -- Góc dưới bên trái
CopyButton.Size = UDim2.new(0, 150, 0, 40)
CopyButton.Text = "Copy Discord Link"
CopyButton.TextColor3 = Color3.new(1, 1, 1)
CopyButton.Font = Enum.Font.SourceSans
CopyButton.TextSize = 16

local UICorner2 = Instance.new("UICorner")
UICorner2.CornerRadius = UDim.new(0, 10)
UICorner2.Parent = CopyButton

CopyButton.MouseButton1Click:Connect(function()
    -- Sao chép link Discord
    setclipboard("https://discord.gg/3Tp35BhYeG")
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Thành công!",
        Text = "Link Discord đã được sao chép.",
        Duration = 3
    })
end)

-- Thêm chức năng vào tab "Giới thiệu"
Tabs.ALL:AddButton({
    Title = "Script được tạo bởi Thong",
    Callback = function()
        print("Script đã chạy thành công!")
    end
})

Tabs.ALL:AddButton({
    Title = "Update 1",
    Description = "trạng thái script No Key",
    Callback = function()
        _G.Settings = {
            LowDetail = true,
            Images = { Invisible = true, Destroy = false },
            ["No Particles"] = true,
            ["No Camera Effects"] = true
        }
        print("Boost FPS thành công!")
    end
})

