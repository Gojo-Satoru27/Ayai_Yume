local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Thongdepzai2701/Ayai_Yume/refs/heads/main/Thongontop.txt')))()

local Window = OrionLib:MakeWindow({Name = "Script Tổng hợp", HidePremium = false, SaveConfig = true, ConfigFolder = "dxl_bf"})

-- Thông báo khi bật script
OrionLib:MakeNotification({
	Name = "Script  made by Thong",
	Content = "Join Discord: https://discord.gg/3Tp35BhYeG để lấy script mới nhất.",
	Image = "rbxassetid://4483345998",
	Time = 60 -- Thời gian thông báo hiển thị (giây)
})

-- Tab Main(Tap1)
local TabMain = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://97129433129151",
	PremiumOnly = false
})

-- Các nút trong Tab Main
TabMain:AddButton({
	Name = "Check FPS",
	Callback = function()
		print("FPS hiện tại: " .. tostring(1 / wait()))
	end
})

TabMain:AddButton({
	Name = "Reset Character",
	Callback = function()
		local player = game.Players.LocalPlayer
		if player.Character then
			player.Character:BreakJoints()
		end
	end
})

TabMain:AddButton({
	Name = "Enable Shift Lock",
	Callback = function()
		game.Players.LocalPlayer.DevEnableMouseLock = true
		print("Shift Lock đã được bật!")
	end
})

TabMain:AddButton({
	Name = "Change WalkSpeed",
	Callback = function()
		local player = game.Players.LocalPlayer
		if player.Character and player.Character:FindFirstChild("Humanoid") then
			player.Character.Humanoid.WalkSpeed = 50
			print("WalkSpeed đã đổi thành 50!")
		end
	end
})

-- Thêm script bay (fly script)
TabMain:AddButton({
	Name = "Fly Script",
	Callback = function()
		-- Script bay
		local player = game.Players.LocalPlayer
		local mouse = player:GetMouse()
		local flying = false
		local speed = 50
		local bodyGyro, bodyVelocity

		mouse.KeyDown:Connect(function(key)
			if key == "e" then
				if not flying then
					flying = true
					local character = player.Character or player.CharacterAdded:Wait()
					local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
					
					bodyGyro = Instance.new("BodyGyro", humanoidRootPart)
					bodyGyro.P = 9e4
					bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
					bodyGyro.CFrame = humanoidRootPart.CFrame

					bodyVelocity = Instance.new("BodyVelocity", humanoidRootPart)
					bodyVelocity.Velocity = Vector3.zero
					bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)

					while flying do
						wait()
						bodyGyro.CFrame = humanoidRootPart.CFrame
						bodyVelocity.Velocity = humanoidRootPart.CFrame.LookVector * speed
					end
				else
					flying = false
					if bodyGyro then bodyGyro:Destroy() end
					if bodyVelocity then bodyVelocity:Destroy() end
				end
			elseif key == "r" and flying then
				speed = speed + 10
				print("Speed tăng: " .. speed)
			elseif key == "f" and flying then
				speed = speed - 10
				print("Speed giảm: " .. speed)
			end
		end)
	end
})

