end

    }

    -- Hàm kiểm tra key
    local function checkKeyAndExecute(scriptLink)
        if keyEntered then
            loadstring(game:HttpGet(scriptLink))()
        else
            print("Please enter the correct key.")
        end
    end

    -- Thêm nút với chức năng kiểm tra key
    for _, script in ipairs(scripts) do
        AddButton(Tab1o, {
            Name = script.Name,
            Callback = function() 
                -- Kiểm tra key trước khi chạy script
                local enteredKey = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("KeyInputField").Text
                if enteredKey == correctKey then
                    keyEntered = true
                    checkKeyAndExecute(script.Link)
                else
                    print("Incorrect key.")
                end
            end
        })
    end
