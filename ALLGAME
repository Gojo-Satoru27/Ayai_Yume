-- Tải và chạy thư viện Fluent
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

-- Tải các tiện ích bổ sung
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Tạo cửa sổ chính
local Window = Fluent:CreateWindow({
    Title = "Script Tổng Hợp",
    SubTitle = "made by Thong",
    TabWidth = 160,
    Size = UDim2.fromOffset(500, 360),
    Acrylic = true,
    Theme = "Dark",  -- Đặt lại chủ đề sang "Dark" nếu không hỗ trợ "niger"
    MinimizeKey = Enum.KeyCode.End
})

-- Tạo các tab
local Tabs = {
    ALL = Window:AddTab({ Title = "All Map", Icon = "" }),
    BloxFruits = Window:AddTab({ Title = "Blox Fruits", Icon = "" }),
    KL = Window:AddTab({ Title = "King Legacy", Icon = "" }),
    AD = Window:AddTab({ Title = "Anime Defense", Icon = "" }),
    AV = Window:AddTab({ Title = "Anime Venguard", Icon = "" }),
    AR = Window:AddTab({ Title = "Reborn", Icon = "" }),
    MS = Window:AddTab({ Title = "Meme Sea", Icon = "" }),
    D = Window:AddTab({ Title = "Doors", Icon = "" }),
    F = Window:AddTab({ Title = "Fisch", Icon = "" }),
    PG = Window:AddTab({ Title = "Pets Go", Icon = "" }),
    E = Window:AddTab({ Title = "Evade", Icon = "" })
}

-- Tạo GUI chính
local ScreenGui = Instance.new("ScreenGui")
local ImageButton = Instance.new("ImageButton")
local UICorner = Instance.new("UICorner")

-- Cài đặt GUI
ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Cấu hình ImageButton
ImageButton.Parent = ScreenGui
ImageButton.BackgroundTransparency = 1
ImageButton.ImageColor3 = Color3.fromRGB(0, 0, 0) -- Màu đen
ImageButton.BorderSizePixel = 0
ImageButton.Position = UDim2.new(0.10083337, 0, 0.09, 0) -- Vị trí
ImageButton.Size = UDim2.new(0, 50, 0, 50) -- Kích thước
ImageButton.Draggable = true
icon = "rbxthumb://type=Asset&id=106431844691075&w=150&h=150" -- Đặt ID hình ảnh của bạn ở đây
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = ImageButton

-- Thêm sự kiện cho nút
ImageButton.MouseButton1Down:Connect(function()
    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.End, false, game)
end)

-- Thêm chức năng vào tab "MENU"
Tabs.ALL:AddButton({
    Title = "Script được tạo bởi Thong",
    Callback = function()
        print("Script đã chạy thành công!")
    end
})

Tabs.ALL:AddButton({
    Title = "Update 1",
    Description = "trạng thái script No Key",
    Callback = function()
        _G.Settings = {
            LowDetail = true,
            Images = {
                Invisible = true,
                Destroy = false
            },
            ["No Particles"] = true,
            ["No Camera Effects"] = true
        }
        print("Boost FPS thành công!")
    end
})

-- Thêm các nút vào tab "Blox Fruits"
Tabs.BloxFruits:AddButton({
    Title = "Alchemy Hub",
    Description = "No Key",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/FPSBooster.lua"))()
    end
})

Tabs.BloxFruits:AddButton({
    Title = "[New] Redz Hub",
    Description = "No Key",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/BloxFruits/refs/heads/main/Source.lua"))()
    end
})

Tabs.BloxFruits:AddButton({
    Title = "Andepzai Hub Ui Giống Maru",
    Description = "No Key",
    Callback = function()
        repeat wait() until game:IsLoaded() and game.Players.LocalPlayer 
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AndepzaiHubBeta/ref/main/AndepzaiNewUpdated.lua"))()
    end
})

Tabs.BloxFruits:AddButton({
    Title = "W Azure",
    Description = "No Key",
    Callback = function()
        getgenv().Team = "Pirates"
        getgenv().FixCrash = false
        getgenv().FixCrash2 = false
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3b2169cf53bc6104dabe8e19562e5cc2.lua"))()
    end
})

-- Các nút khác tương tự đã sửa cú pháp, nếu bạn cần thêm hãy làm theo mẫu trên
-- Ví dụ:
-- Tabs.BloxFruits:AddButton({
--     Title = "Tên nút",
--     Description = "Mô tả",
--     Callback = function()
--         -- Mã khi nhấn nút
--     end
-- })
